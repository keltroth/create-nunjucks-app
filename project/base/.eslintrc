{
  "extends": "airbnb-base",
  // Ajoute quelques règles par défaut
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      2,
      4
    ],
    "object-curly-spacing": 0,
    "import/extensions": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0
  }
}
